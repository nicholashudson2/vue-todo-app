{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "@vue/cli": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "axios": "^0.19.2",
    "babel-core": "^6.26.3",
    "bootstrap-vue": "^2.4.0",
